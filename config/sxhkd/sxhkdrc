# Letters
super + shift + q
	prompt "Exit X?" "killall Xorg"
super + w
	urlgoto
super + e
	$TERMINAL -e $FILE
super + r
	audio toggle
super + t
	tor
super + y
	$TERMINAL -e calcurse -D ~/.config/calcurse
super + u
	$TERMINAL -e $EDITOR ~/docs/wiki/scratch.wiki
super + o
	open ~ 'repos vidya zig-cache WhatsApp'
super + shift + o
	open /media 'steam gsil'
super + p
	mpc toggle
super + shift + p
	pauseall
super + a
	$TERMINAL -e qalc
super + shift + a
	anki
super + s
	qplay library
super + shift + s
	slack
super + d
	rofi -show run
super + shift + d
	discord
super + x
	lockscreen
super + shift + x
	prompt "Shutdown computer?" "sudo -A shutdown -h now"
super + {_,shift +} c
	{show,qr}clip
super + v
	$TERMINAL -e ncmpcpp -s visualizer
super + {_, shift +} b
	$BROWSER {$HOMEPAGE, --new-window $HOMEPAGE}
super + n
	$TERMINAL -e newsboat && dwmbar rss
super + shift + n
	rssping
super + m
	$TERMINAL -e ncmpcpp
super + shift + m
	audio vol mute

# Etc
super + comma
	qunicode
super + {_,shift +} period
	{qcopy,qpass}
super + Print
	scrotpick
super + {_, shift +} grave
	ib {toggle, power}

# Function keys
super + F1
	manview
super + {_,shift +} F2
	setbg {pick,random}
super + {_,shift +} F9
	dmenu{mount,umount}

# Caps Lock
super + Escape
	warncaps

# Audio
super + {_,shift +} BackSpace
	mpc {prev,next}
super + {minus,plus}
	audio vol 2{-,+}

# XF86 keys
XF86AudioMute
	audio vol mute
XF86Audio{Lower,Raise}Volume
	audio vol 2{-,+}
XF86Audio{Play,Stop,Next,Prev}
	mpc {toggle,pause,next,prev}
XF86MonBrightness{Down,Up}
	xbacklight {-dec,-inc} 15
