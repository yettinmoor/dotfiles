# Term
super + Return
  $TERMINAL
super + shift + Return
  samedir

# Letters
super + shift + q
  prompt "Exit X?" "killall Xorg"
super + w
  urlparse -d | xargs -r0 $BROWSER
super + e
  $TERMINAL -e $FILE
super + t
  tor
super + o
  open
super + {_,shift +} p
  mpc {toggle,pause}
super + a
  $TERMINAL -e python -q
super + shift + a
  $TERMINAL -e pulsemixer
super + s
  $TERMINAL -e calcurse -D ~/.config/calcurse
super + d
  dmenu_run
super + x
  lockscreen
super + shift + x
  prompt "Shutdown computer?" "sudo -A shutdown -h now"
super + c
  plumb
super + b
  $BROWSER $HOMEPAGE
super + n
  $TERMINAL -e newsboat; dwmbar rss
super + m
  $TERMINAL -f mono:pixelsize=19 -e ncmpcpp
super + shift + m
  audio truemute

# Etc
super + comma
  dmenuunicode
super + {_,shift +} period
  {qcopy, qpass}
super + section
  qwikt
super + {_,shift +} Insert
  {showclip, qrclip}
super + Print
  maimpick

# Function keys
super + F1
  manview
super + {_,shift +} F2
  setbg {-c,-r}
super + F3
  displayselect
super + F4
  timer
super + {F9,F10}
  {dmenumount,dmenuumount}

# Audio
super + {_,shift +} BackSpace
  mpc {prev,next}
super + {minus,plus}
  audio vol {-,+}5

# XF86 keys
XF86AudioMute
  audio mute
XF86Audio{Lower,Raise}Volume
  audio vol {-,+}5
XF86Audio{Play,Stop,Next,Prev}
  mpc {toggle,pause,next,prev}
XF86MonBrightness{Down,Up}
  xbacklight {-dec,-inc} 15
